{
  "name": "mys3browser",
  "version": "0.2.2",
  "description": "A Modern S3 Browser Chrome Extension",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && zip -r MyS3Browser-$(node -p \"require('./package.json').version\").zip dist/ -x 'dist/.vite/*'",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "launch-extension": "node scripts/launch-extension.js",
    "screenshots": "node scripts/screenshot.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@types/chrome": "^0.0.254",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/ui": "^4.0.18",
    "happy-dom": "^20.4.0",
    "puppeteer": "^24.15.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shishirsharma/MyS3Browser.git"
  },
  "bugs": {
    "url": "https://github.com/shishirsharma/MyS3Browser/issues"
  },
  "homepage": "https://github.com/shishirsharma/MyS3Browser"
}
